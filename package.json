{
  "name": "work-timer",
  "displayName": "Work-Timer",
  "description": "This is a simple timer for track your work",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*","onExit"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "my-exit-confirmation.confirmExit",
        "title": "Confirm Exit"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}